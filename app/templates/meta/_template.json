{
    "id": "<%= template_id %>",
    "name": "<%= template_name %>",
    "mobile": true,
    "notes": "My very first template!",
    "syntax": 4,
    "version": 1.0,
    "style": {
        "replacements": [
            {
                "type": "color",
                "name": "Background Color",
                "find": "#FFFFFE"
            }
        ]
    },
    "form": {
        "title": {
            "css": {
                "font-family": "Helvetica, Arial, sans-serif",
                "font-size": "18pt;",
                "letter-spacing": "0em;"
            },
            "html": "<strong>Enter your email address below to reserve _______... it's 100% FREE!</strong>"
        },
        "submit": {
            "css": {
                "background-color": "#ffcb01;",
                "border-color": "#ffb403;",
                "border-width": "0pt;",
                "border-style": "solid;",
                "font-family": "Helvetica, Arial, sans-serif",
                "font-size": "14pt;",
                "letter-spacing": "-0.1em;"
            },
            "html": "Free Instant Access &raquo;"
        },
        "privacy": {
            "css": {
                "font-family": "Helvetica, Arial, sans-serif",
                "font-size": "12pt;",
                "letter-spacing": "-0.1em;"
            },
            "html": "<div><i class=\"icon-lock\"></i>We hate SPAM and promise to keep your email address safe. </div>"
        }
    },
    "elements": [
        {
            "id": "logo",
            "name": "Logo",
            "type": "image"
        }
    ],
    "variables": {}
}